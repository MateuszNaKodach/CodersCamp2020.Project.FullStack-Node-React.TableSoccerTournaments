    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.22",
    "@types/nodemailer": "^6.4.0",
    "@types/pino": "^6.3.5",
    "@types/shelljs": "^0.8.8",
