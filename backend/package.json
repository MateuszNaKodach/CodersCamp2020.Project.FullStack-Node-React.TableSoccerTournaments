    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.0.9",
    "nodemon": "^2.0.7",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
