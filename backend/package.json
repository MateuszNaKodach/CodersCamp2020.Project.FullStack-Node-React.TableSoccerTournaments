{
  "name": "table-soccer-tournaments-backend",
  "version": "0.3.0",
  "description": "Table Soccer Tournaments REST API",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf .cache && rimraf coverage && rimraf dist && rimraf node_modules",
    "prebuild": "rimraf dist",
    "build": "ts-node build.ts && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:ci": "prettier -c \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --format codeframe",
    "lint:ci": "eslint \"{src,apps,libs,test}/**/*.ts\" --format codeframe",
    "prestart": "ts-node build.ts && tsc",
    "start": "node dist/index.js",
    "start:dev": "nodemon",
    "test": "jest --testRegex \"[.](spec|test)[.]ts\"",
    "test:unit": "jest --testRegex \"[.](spec|test)[.]ts\"",
    "test:all": "jest --testRegex \"[.](spec|test|mongo-spec|postgres-spec|mailhog-spec)[.]ts\"",
    "test:mongo": "jest --testRegex \"[.](mongo-spec)[.]ts\"",
    "test:postgres": "jest --testRegex \"[.](postgres-spec)[.]ts\"",
    "test:mailhog": "jest --testRegex \"[.](mailhog-spec)[.]ts\"",
    "test:watch": "jest --watchAll",
    "test:cov": "jest --coverage",
    "test:ci": "jest --testRegex \"[.](spec|test|mongo-spec|postgres-spec|mailhog-spec)[.]ts\" --ci --silent --coverage"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "config": "^3.3.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-openapi-validator": "^4.10.9",
    "express-winston": "^4.0.5",
    "google-auth-library": "^7.0.4",
    "helmet": "^4.4.1",
    "http-status-codes": "^2.1.4",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "jsonwebtoken": "^8.5.1",
    "mailhog-awesome": "^0.1.4",
    "mongoose": "^5.11.16",
    "nodemailer": "^6.5.0",
    "pg": "^8.5.1",
    "pino": "^6.11.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.31",
    "uuid": "^8.3.0",
    "winston": "^3.3.3",
    "yamljs": "^0.3.0"
  },
